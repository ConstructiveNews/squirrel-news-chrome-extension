{
  "manifest_version": 3,
  "name": "Squirrel News Extension",
  "version": "1.0.0",
  "description": "Chrome extension for Squirrel News",
  "author": "Korotkaya Irina",
  "action":{
      "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "search"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
 }
  
}